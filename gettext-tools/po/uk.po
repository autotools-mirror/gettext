msgstr "      --use-fuzzy             використовувати неточні записи\n"

#: src/msgcmp.c:246
#, c-format
msgid "      --use-untranslated      consider untranslated entries\n"
msgstr "      --use-untranslated      використовувати неперекладені записи\n"

