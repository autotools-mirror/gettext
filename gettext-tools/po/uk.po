msgid "Peter Miller"
msgstr "Peter Miller"

#: src/msgcmp.c:189 src/msgmerge.c:390
#, c-format
msgid "no input files given"
msgstr "не вказані вхідні файли"

#: src/msgcmp.c:194 src/msgmerge.c:395
