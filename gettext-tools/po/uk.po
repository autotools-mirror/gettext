#, c-format
msgid "  -i, --indent                indented output style\n"
msgstr "  -i, --indent                стиль виводу з відступами\n"

#: src/msgconv.c:367 src/msgen.c:370 src/msgfilter.c:492 src/msggrep.c:605
#: src/msgmerge.c:651
#, c-format
