#, c-format
msgid "Language specific options:\n"
msgstr "Voľby špecifické pre jednotlivé jazyky:\n"

#: src/xgettext.c:1227
#, c-format
msgid "  -a, --extract-all           extract all strings\n"
msgstr "  -a, --extract-all           extrahovať všetky reťazce\n"

#: src/xgettext.c:1229
#, c-format
msgid ""
"                                (only languages C, C++, ObjectiveC, Python,\n"
"                                Java, C#, JavaScript, TypeScript, TSX, Scheme,\n"
"                                Guile, Lisp, EmacsLisp, librep, Rust, Go, Shell,\n"
"                                awk, Lua, Modula-2, D, Vala, Tcl, Perl, PHP,\n"
"                                GCC-source, Glade, GSettings)\n"
msgstr ""
"                                (len jazyky C, C++, ObjectiveC, Python,\n"
"                                Java, C#, JavaScript, TypeScript, TSX, Scheme,\n"
"                                Guile, Lisp, EmacsLisp, librep, Rust, Go, Shell,\n"
"                                awk, Lua, Modula-2, D, Vala, Tcl, Perl, PHP,\n"
"                                GCC-source, Glade, GSettings)\n"

#: src/xgettext.c:1238
#, c-format
msgid ""
"                                (only languages C, C++, ObjectiveC, Python,\n"
"                                Java, C#, JavaScript, TypeScript, TSX, Scheme,\n"
"                                Guile, Lisp, EmacsLisp, librep, Rust, Go, Shell,\n"
