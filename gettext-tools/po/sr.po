#: src/xgettext.c:1239
#, c-format
msgid ""
"      --no-check=NAME         don't perform syntax check on messages\n"
"                                (url, email)\n"
msgstr ""
"      --no-check=НАЗИВ        не врши проверу синтаксе у порукама\n"
"                                (јрл, е-пошта)\n"

#: src/xgettext.c:1242
#, c-format
msgid ""
"      --sentence-end=TYPE     type describing the end of sentence\n"
"                                (single-space, which is the default, \n"
"                                 or double-space)\n"
msgstr ""
"      --sentence-end=ВРСТА    врста која описује крај реченице\n"
"                                (један-размак, који је основни, \n"
"                                 или двоструки-размак)\n"

#: src/xgettext.c:1247
#, c-format
msgid "Language specific options:\n"
msgstr "Посебне наредбе језика:\n"

#: src/xgettext.c:1249
#, c-format
